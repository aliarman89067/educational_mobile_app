{
  "expo": {
    "name": "Mirror",
    "slug": "mirror",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.aliarman.mirror",
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aliarman.mirror"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/adaptive-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/OpenSans-Regular.ttf",
            "./assets/fonts/OpenSans-Medium.ttf",
            "./assets/fonts/OpenSans-Light.ttf",
            "./assets/fonts/OpenSans-ExtraBold.ttf",
            "./assets/fonts/OpenSans-Bold.ttf"
          ]
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.791454953921-46gv6gs1f5shlec7elrmiecb0gv47vpn"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "fe391e7e-7cb6-4b82-838b-df31932efc4a"
      }
    }
  }
}
